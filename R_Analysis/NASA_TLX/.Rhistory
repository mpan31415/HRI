anova_single_trust <- get_anova_table(res.aov)
anova_single_trust
######################### SIG 2-WAY INTER: Pairwise comparisons between group levels #########################
pwc <- data_df %>%
group_by(autonomy) %>%
pairwise_t_test(single_trust ~ order, p.adjust.method = "bonferroni")
pwc
